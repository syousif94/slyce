{
  "expo": {
    "name": "Slyce",
    "slug": "slyce",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "me.syousif.slyce"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "me.syousif.slyce"
    }
  }
}
